<h3>WEAPONS:</h3>
<ul id="example-list" class="list-group">
  {{#each each_weapon}}
  <li data-id="{{this.id}}" class="list-group-item">
    {{!-- a href="product/{{this.id} refers to the app.get in htmlRoutes with the html route "/product/:id" --}}
    <a href="product/{{this.id}}">{{this.name}} : &quot{{this.text}}" &nbsp price: ${{this.price}} &nbsp amount in
      stock: {{this.quantity}}</a>
    <button class="btn btn-danger float-right delete">add to cart</button>
  </li>
  {{/each}}
</ul>
